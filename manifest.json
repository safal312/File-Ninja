{
    "manifest_version": 2,
    "name": "File Ninja",
    "description": "Enter the code generated from our File Ninja App to instantly download all required files",
    "version": "1.0.0",
    "icons": {
        "128": "main.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "main.png",
        "default_popup": "page/popup.html",
        "default_title": "Access your files!"
    },
    "permissions": [
        "downloads",
        "tabs"
    ]
}
